{
  "name": "my-react-admin",
  "version": "1.0.0",
  "scripts": {
    "watch-dashboard": "wp-scripts start --config webpack.dashboard.config.js",
    "build-dashboard": "wp-scripts build --webpack-src-dir=source/dashboard --output-path=assets/js/dashboard/ --config webpack.dashboardBuild.config.js",
    "watch-sass": "sass --watch source/assets/sass:assets/css",
    "build-sass": "sass source/assets/sass:assets/css",
    "build-postcss": "postcss assets/css/*.css --use autoprefixer cssnano -d assets/css --no-map",
    "start": "concurrently \"npm:watch-*\"",
    "build": "concurrently \"npm:build-*\""
  },
  "dependencies": {
    "@wordpress/components": "^23.0.0",
    "@wordpress/element": "^2.0.0",
    "@wordpress/i18n": "^2.0.0",
    "react-router-dom": "^7.5.1"
  },
  "devDependencies": {
    "@wordpress/scripts": "^30.7.0",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.1.2",
    "cssnano": "^7.0.7",
    "glob": "^11.0.2",
    "postcss": "^8.5.3",
    "postcss-cli": "^11.0.1"
  }
}
